{
  "name": "@smart-bazar/shop",
  "version": "1.0.1",
  "private": true,
  "config": {
    "appName": "Altudo",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreDistPath": "/dist/Altudo",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "language": "en"
  },
  "scripts": {
    "clean": "rimraf \"{.next,node_modules,__generated__,.cache,src/framework/graphql/**/*.d.ts}\"",
    "dev:rest": "cross-env NODE_OPTIONS='--inspect' next dev -p 3003",
    "build:rest": "next build",
    "codegen": "node -r dotenv/config $(yarn bin)/graphql-let",
    "dev:gql": "yarn codegen && cross-env NODE_OPTIONS='--inspect' next dev -p 3001",
    "build:gql": "yarn codegen && next build",
    "start": "next start -p 3003",
    "lint": "next lint",
    "postbuild:rest": "cross-env next-sitemap",
    "prepare": "cd ../../.. && husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "graphql:update": "ts-node --project tsconfig.scripts.json ./scripts/fetch-graphql-introspection-data.ts",
    "jss": "jss",
    "scaffold": "ts-node --project tsconfig.scripts.json scripts/scaffold-component.ts",
    "start:sitecore": "cross-env-shell JSS_MODE=disconnected \"npm-run-all --parallel dev:rest start:disconnected-proxy start:watch-components\"",
    "start:connected": "npm-run-all --parallel dev:rest start:watch-components",
    "start:disconnected-proxy": "ts-node --project tsconfig.scripts.json ./scripts/disconnected-mode-proxy.ts",
    "start:production": "npm-run-all build:rest start",
    "start:watch-components": "ts-node --project tsconfig.scripts.json scripts/generate-component-factory.ts --watch",
    "update-component-factory": "ts-node --project tsconfig.scripts.json scripts/generate-component-factory.ts"
  },
  "dependencies": {
